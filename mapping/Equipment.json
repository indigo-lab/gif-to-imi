{
  "@context": "https://imi.go.jp/ns/core/context.jsonld",
  "@type": "設備型",
  "ID": {
    "@type": "ID型",
    "体系": { "@type": "ID体系型", "種別": "設備ID" },
    "識別値": "/id"
  },
  "種別コード/typeInformation": { "$ref": "CodeInformationModel.json" },
  "名称": {
    "@type": "名称型",
    "表記": "/name",
    "カナ表記": "/nameKana",
    "ローマ字表記": "/nameEn"
  },
  "説明": ["/description", "/remark"],
  "設置地点/equipmentAddress": { "@type": "地物型", "住所": { "$ref": "Address.json" } },
  "記述": [
    { "@type": "記述型", "種別": "状態", "説明": "/status" },
    { "@type": "記述型", "種別": "利用可能曜日", "説明": "/availableDay" },
    { "@type": "記述型", "種別": "一般利用可否", "説明": "/generalUserAcceptanceStatus" }
  ],
  "利用可能時間": {
    "@type": " イベントスケジュール型",
    "開始時間": "/startTime",
    "終了時間": "/endTime",
    "説明": ["/availabileDateTimeDescription", "/availabileDateTimeFormula"]
  },
  "参照": { "@type": "参照型", "種別": "URL", "参照先": "/url" },
  "連絡先/contactPointInformation": { "$ref": "ContactPoint.json" },
  "設置地点/relatedFacility": { "$ref": "Facility.json" },
  "設置地点/relatedBuilding": { "@type": "施設型", "建物": { "$ref": "Building.json" } }
}
